{
  "expo": {
    "name": "Virtual Bazaar",
    "slug": "VirtualBazaar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "splash": {
      "image": "./assets/splashscreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notifications.png",
          "color": "#ff4500",
          "androidCollapsedTitle": "Virtual Bazaar"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyBbzt5t9rg2kCEIBPn8_OGW1Y50tHGJ6sk"
      },
      "bundleIdentifier": "com.maazkhansw.VirtualBazaar"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true,
      "package": "com.maazkhansw.VirtualBazaar",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBbzt5t9rg2kCEIBPn8_OGW1Y50tHGJ6sk"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Advance e-commerce platform empowered by computer vision and augmented reality"
  }
}
